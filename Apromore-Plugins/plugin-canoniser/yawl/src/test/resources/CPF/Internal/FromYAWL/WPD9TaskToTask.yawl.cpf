<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:CanonicalProcess xmlns:ns2="http://www.apromore.org/CPF" uri="WPD9TaskToTask" version="0.1" name="WPD9TaskToTask" author="Felix" rootIds="N-Net">
    <Net id="N-Net" originalID="Net">
        <Node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:EventType" id="C-InputCondition" originalID="InputCondition">
            <name>IN</name>
        </Node>
        <Node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:TaskType" id="C-A" originalID="A">
            <name>A</name>
            <attribute name="http://www.yawlfoundation.org/yawlschema/outputParam">
                <outputParam:outputParam xmlns:outputParam="http://www.yawlfoundation.org/yawlschema" xmlns="http://www.yawlfoundation.org/yawlschema">
                    <index>0</index>
                    <name>x</name>
                    <type>string</type>
                    <namespace>http://www.w3.org/2001/XMLSchema</namespace>
                    <defaultValue>true</defaultValue>
                </outputParam:outputParam>
            </attribute>
            <objectRef id="id809b5ebc-ad13-46df-adf2-6824a7940b42" objectId="D-N-Net-x" type="output" optional="false" consumed="true"/>
            <outputExpr>
                <language>http://www.w3.org/2005/xpath-functions/</language>
                <expression>x = Boolean({cpf:getTaskObjectValue('x')/text()})</expression>
            </outputExpr>
            <teamWork>false</teamWork>
        </Node>
        <Node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:TaskType" id="C-B" originalID="B">
            <name>B</name>
            <attribute name="http://www.yawlfoundation.org/yawlschema/inputParam">
                <inputParam:inputParam xmlns:inputParam="http://www.yawlfoundation.org/yawlschema" xmlns="http://www.yawlfoundation.org/yawlschema" optional="true">
                    <index>0</index>
                    <name>y</name>
                    <type>boolean</type>
                    <namespace>http://www.w3.org/2001/XMLSchema</namespace>
                </inputParam:inputParam>
            </attribute>
            <objectRef id="id9f5995f8-e14a-4cee-ab8b-c8a997853ce6" objectId="D-N-Net-x" type="input" optional="false" consumed="true"/>
            <inputExpr>
                <language>http://www.w3.org/2005/xpath-functions/</language>
                <expression>y = {cpf:getObjectValue('x')}</expression>
            </inputExpr>
            <teamWork>false</teamWork>
        </Node>
        <Node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:EventType" id="C-OutputCondition" originalID="OutputCondition">
            <name>OUT</name>
        </Node>
        <Edge id="C-InputCondition-A" default="true" sourceId="C-InputCondition" targetId="C-A"/>
        <Edge id="C-A-B" default="true" sourceId="C-A" targetId="C-B"/>
        <Edge id="C-B-OutputCondition" default="true" sourceId="C-B" targetId="C-OutputCondition"/>
        <Object xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns2:SoftType" id="D-N-Net-x" originalID="N-Net_x">
            <name>x</name>
            <attribute name="http://www.yawlfoundation.org/yawlschema/localVariable">
                <localVariable:localVariable xmlns:localVariable="http://www.yawlfoundation.org/yawlschema" xmlns="http://www.yawlfoundation.org/yawlschema">
                    <index>0</index>
                    <name>x</name>
                    <type>boolean</type>
                    <namespace>http://www.w3.org/2001/XMLSchema</namespace>
                    <initialValue>true</initialValue>
                </localVariable:localVariable>
            </attribute>
            <type>boolean</type>
        </Object>
    </Net>
</ns2:CanonicalProcess>
