<listbox id="processSummariesListbox" hflex="true" vflex="true" span="true" autopaging="true" mold="default" multiple="true" checkmark="false">
    <listhead sizable="false">
        <listheader                           width="32px" visible="true" />
        <listheader label="Score"             hflex="1"    visible="true" id="columnScore"/>
        <listheader label="Name"              hflex="5"/>                   <!-- name -->
        <listheader label="Id"                hflex="1"    align="center"/> <!-- id -->
        <listheader label="Format"            hflex="1"/>                 <!-- originalNativeType -->
        <listheader label="Domain"            hflex="1"/>                   <!-- domain -->
        <listheader label="Ranking"           width="90px" align="center"/> <!-- ranking -->
        <listheader label="Latest version"    hflex="min"  align="center"/> <!-- lastVersion -->
        <listheader label="Owner"             hflex="1"/>                   <!-- owner -->
    </listhead>
</listbox>
