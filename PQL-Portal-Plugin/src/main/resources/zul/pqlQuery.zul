<window id="win" width="600px" closable="false" position="center" title="PQL Query">
  <vbox>
    <groupbox width="585px">
      <caption label="Query"/>
      <textbox name="query-name" id="query" width="570px" />
    </groupbox>
    <div align="center">
      <button id="cancelButton" label="Cancel" image="img/cancel2.png"></button>
      <button id="okButton"     label="OK"     image="img/ok.png"></button>
    </div>
  </vbox>
</window>
