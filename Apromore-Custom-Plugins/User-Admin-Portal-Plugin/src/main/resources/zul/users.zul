<window width="95%" position="center" title="Users">
    <vbox>
        <listbox id="usersListbox">
            <listhead>
                <listheader label="Username" hflex="1"/>
                <listheader label="First name" hflex="1"/>
                <listheader label="Last name" hflex="1"/>
                <listheader label="Date created" hflex="1"/>
                <listheader label="Last activity date" hflex="1"/>
            </listhead>
            <template name="model">
                <listitem>
                    <listcell label="${each.username}"/>
                    <listcell label="${each.firstName}"/>
                    <listcell label="${each.lastName}"/>
                    <listcell label="${each.dateCreated}"/>
                    <listcell label="${each.lastActivityDate}"/>
                </listitem>
            </template>
        </listbox>
        <div align="center">
            <button id="okButton" label="OK" image="img/ok.png"></button>
        </div>
    </vbox>
</window>
