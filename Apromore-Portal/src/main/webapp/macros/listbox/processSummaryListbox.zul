<listbox id="processSummariesListbox" vflex="true" span="true" autopaging="true" mold="paging" multiple="true" checkmark="false">
    <listhead sizable="true" >
        <listheader width="25px" visible="true" />
        <listheader width="min"                label="Score" id="columnScore" visible="true" />
        <listheader                            label="Name"              sort="auto(name)" />
        <listheader width="min" align="center" label="Id"                sort="auto(id)" />
        <listheader width="10em"               label="Original language" sort="auto(originalNativeType)" />
        <listheader width="min"                label="Domain"            sort="auto(domain)" />
        <listheader width="6em" align="center" label="Ranking"           sort="auto(ranking)" />
        <listheader width="6em" align="center" label="Latest version"    sort="auto(lastVersion)" />
        <listheader width="min"                label="Owner"             sort="auto(owner)" />
    </listhead>
</listbox>
