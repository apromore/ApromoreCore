<listbox id="logSummariesListbox" vflex="true" span="true" autopaging="true" mold="paging" multiple="true" checkmark="false">
    <listhead sizable="true" >
        <listheader width="25px" visible="true" />
        <listheader hflex="min" label="Score" id="columnScore" visible="true" />
        <listheader hflex="min" label="Name" sort="auto(name)" />
        <listheader hflex="min" label="Id" sort="auto(id)" />
        <listheader hflex="min" label="Domain" sort="auto(domain)" />
        <listheader hflex="min" label="Ranking" sort="auto(ranking)" />
        <listheader hflex="min" label="Owner" sort="auto(owner)" />
    </listhead>
</listbox>
